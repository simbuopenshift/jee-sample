public class Main {
    public static void main(String[] args) {
        String input = "example.string.value";
        String result = getValueAfterSecondDot(input);
        System.out.println("Value after the second dot: " + result);
    }

    public static String getValueAfterSecondDot(String input) {
        int firstDotIndex = input.indexOf('.');
        if (firstDotIndex != -1) {
            int secondDotIndex = input.indexOf('.', firstDotIndex + 1);
            if (secondDotIndex != -1) {
                return input.substring(secondDotIndex + 1);
            }
        }
        return ""; // or handle the case where there is no second dot
    }
}
